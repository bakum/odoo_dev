<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_distrib_options" model="ir.sequence">
            <field name="name">Distributors sequence</field>
            <field name="code">distrib.distributors</field>
            <field name="prefix">DS</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_distrib_move_adj" model="ir.sequence">
            <field name="name">Distributors move sequence adj</field>
            <field name="code">distrib.distributors.move.adj</field>
            <field name="prefix">ADJ/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_distrib_move_in" model="ir.sequence">
            <field name="name">Distributors move sequence in</field>
            <field name="code">distrib.distributors.move.in</field>
            <field name="prefix">IN/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_distrib_move_out" model="ir.sequence">
            <field name="name">Distributors move sequence out</field>
            <field name="code">distrib.distributors.move.out</field>
            <field name="prefix">OUT/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="sequence_distrib_marketing_exp" model="ir.sequence">
            <field name="name">Distributors Marketing Expenses</field>
            <field name="code">distrib.marketing.exp</field>
            <field name="prefix">EXP/%(year)s/%(month)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
