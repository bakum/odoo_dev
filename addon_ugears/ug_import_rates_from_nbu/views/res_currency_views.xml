<?xml version="1.0" encoding="utf-8" ?>

<odoo>
    <data>
        <record id="view_res_currency_inherit_ug_tree" model="ir.ui.view">
            <field name="name">res.currency.tree.inherit.ug</field>
            <field name="model">res.currency</field>
            <field name="inherit_id" ref="base.view_currency_tree"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//tree" position="attributes">
                        <attribute name="js_class">res_currency_list</attribute>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>
